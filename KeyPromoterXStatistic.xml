<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Data Source Properties">
        <Item>
          <IdeaActionID>DatabaseView.PropertiesAction</IdeaActionID>
          <ShortCut>'⌘I'</ShortCut>
          <Description>Data Source Properties</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B'</ShortCut>
          <Description>Declaration</Description>
          <Count>16</Count>
        </Item>
      </Statistic>
      <Statistic Action="Favorites">
        <Item>
          <IdeaActionID>ActivateFavoritesToolWindow</IdeaActionID>
          <ShortCut>'⌘2'</ShortCut>
          <Description>Favorites</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find in Path...">
        <Item>
          <IdeaActionID>FindInPath</IdeaActionID>
          <ShortCut>'⇧⌘F'</ShortCut>
          <Description>Find in Path...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Console...">
        <Item>
          <IdeaActionID>Jdbc.OpenConsole.Any</IdeaActionID>
          <ShortCut>'⇧⌘F10'</ShortCut>
          <Description>Jump to Console...</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Project">
        <Item>
          <IdeaActionID>ActivateProjectToolWindow</IdeaActionID>
          <ShortCut>'⌘1'</ShortCut>
          <Description>Project</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Set NULL">
        <Item>
          <IdeaActionID>Console.TableResult.SetNull</IdeaActionID>
          <ShortCut>'⌥⌘N'</ShortCut>
          <Description>Set NULL</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Structure">
        <Item>
          <IdeaActionID>ActivateStructureToolWindow</IdeaActionID>
          <ShortCut>'⌘7'</ShortCut>
          <Description>Structure</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Synchronize">
        <Item>
          <IdeaActionID>DatabaseView.SynchronizeAction</IdeaActionID>
          <ShortCut>'⌥⌘Y'</ShortCut>
          <Description>Synchronize</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>