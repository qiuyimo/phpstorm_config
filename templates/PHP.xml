<templateSet group="PHP">
  <template name="f" value="foreach ($ITERABLE$ as $KEY$ =&gt; $VALUE$) {&#10;    $END$&#10;}" description="foreach(iterable_expr as $value) {...}" toReformat="true" toShortenFQNames="true">
    <variable name="ITERABLE" expression="" defaultValue="clipboard()" alwaysStopAt="true" />
    <variable name="KEY" expression="" defaultValue="&quot;$key&quot;" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="&quot;$value&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="d" value="dump($VAR$);&#10;$END$" description="dump($var);" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="clipboard()" alwaysStopAt="true" />
    <context>
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="date" value="date(&quot;$FORMAT$&quot;, $TIME$);&#10;$END$" description="date(&quot;Y-m-d H:i:s&quot;, time());" toReformat="false" toShortenFQNames="true">
    <variable name="FORMAT" expression="" defaultValue="&quot;Y-m-d H:i:s&quot;" alwaysStopAt="true" />
    <variable name="TIME" expression="" defaultValue="&quot;time()&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP Statement" value="true" />
    </context>
  </template>
</templateSet>